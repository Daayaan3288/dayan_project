class Car:
    def __init__(self):
        self.engine_status = "OFF"
    def start_engine(self):
        self.engine_status = "ON"
        print("Engine started.")
    def stop_engine(self):
        self.engine_status = "OFF"
        print("Engine stopped.")
# تست کلاس Car  :]]
my_car = Car()
my_car.start_engine()  # خروجی: Engine started.
my_car.stop_engine()  # خروجی: Engine stopped.
